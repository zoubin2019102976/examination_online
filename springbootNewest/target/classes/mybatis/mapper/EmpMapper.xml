<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.org.nenu.mapper.EmpMapper">
    <select id="selectAll" resultType="com.org.nenu.pojo.Emp">
        SELECT
            emp_id as "empId",
            emp_name as "empName",
            emp_age as "empAge"
        FROM table_emp
    </select>
</mapper>